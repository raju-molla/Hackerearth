
#include <bits/stdc++.h>
using namespace std;


int main() {
	string s;
    getline(cin,s);
    vector<string>v;
    for(int i=0; i<s.size(); i++){
        int res= int(s[i])-32;
        s[i]=char(res);
        //cout<<char(res)<<endl;
    }
    for(int i=0; i<s.size(); i++){
        if(int(s[i])==0){
            cout<<endl;
        }
        else{
            cout<<s[i];
        }
    }

}
